{
	"name": "boilerplate",
	"version": "1.0.0",
	"description": "pearcircuit boilerplace",
	"main": "index.js",
	"scripts": {
		"build": "next build",
		"create-function": "node scripts/create-function.js",
		"create-migration": "node scripts/create-migration.js",
		"create-view": "node scripts/create-view.js",
		"dev": "nodemon --ignore pages server.js",
		"migrate": "node scripts/migrate.js",
		"prestart": "npm i --verbose && npm run migrate && if [ \"${NODE_ENV:-development}\" != development ]; then npm run build; fi",
		"server": "node server.js",
		"start": "npm run dev"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@material-ui/core": "4.3.0",
		"@material-ui/styles": "4.3.0",
		"apollo-boost": "0.4.2",
		"axios": "0.19.0",
		"babel-plugin-inline-dotenv": "1.3.3",
		"babel-plugin-transform-inline-environment-variables": "0.4.3",
		"classnames": "2.2.6",
		"colors": "1.3.3",
		"dotenv": "8.0.0",
		"express": "4.17.1",
		"fs-extra": "8.0.1",
		"graphql-tag": "2.10.1",
		"husky": "2.4.0",
		"lint-staged": "8.2.0",
		"lodash": "4.17.11",
		"mkdirp": "0.5.1",
		"next": "8.1.0",
		"next-routes": "1.4.2",
		"next-with-apollo": "3.4.0",
		"nodemon": "1.19.1",
		"path": "0.12.7",
		"pg": "7.11.0",
		"postgraphile": "4.4.0",
		"postgraphile-plugin-connection-filter": "1.1.0",
		"postgrator": "3.10.2",
		"prettier": "1.18.2",
		"pretty-hrtime": "1.0.3",
		"react": "^16.8.6",
		"react-apollo": "2.5.6",
		"react-dom": "16.8.6",
		"react-jss": "8.6.1",
		"webpack": "4.33.0"
	},
	"lint-staged": {
		"**/*.js": [
			"prettier --write",
			"git add"
		]
	}
}
